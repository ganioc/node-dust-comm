{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": false
    },
    "extends": "standard",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "semi": "warn",
        "space-before-function-paren": "off",
        "camelcase":"off"
    }
}
